[
  {
    "label": "Build & Debug Zed",
    "build": {
      "command": "cargo",
      "args": [
        "build",
        "--bin",
        "lilac"
      ]
    },
    "program": "$ZED_WORKTREE_ROOT/target/debug/lilac",
    "args": [
      "./code.wl"
    ],
    "sourceLanguages": [
      "rust"
    ],
    "request": "launch",
    "adapter": "CodeLLDB",
    "cwd": "$ZED_WORKTREE_ROOT"
  }
]